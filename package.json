{
   "name": "stack-lang",
   "description": "stack language server",
   "version": "0.2.3",
   "repository": "https://github.com/stonum/stack-lang",
   "publisher": "stonum",
   "categories": [
      "Programming Languages",
      "Formatters",
      "Linters"
   ],
   "license": "MIT",
   "keywords": [
      "language-server",
      "stack"
   ],
   "engines": {
      "vscode": "^1.69.0"
   },
   "activationEvents": [
      "onLanguage:stack"
   ],
   "main": "./client/out/extension",
   "contributes": {
      "languages": [
         {
            "id": "stack",
            "aliases": [
               "stack"
            ],
            "extensions": [
               ".prg",
               ".hdl",
               ".rpt"
            ],
            "firstLine": "^#![Ss][Tt][Aa][Cc][Kk]\\b",
            "configuration": "./syntaxes/configuration.json"
         }
      ],
      "grammars": [
         {
            "language": "stack",
            "scopeName": "source.stack",
            "path": "./syntaxes/stack.json",
            "embeddedLanguages": {
               "meta.embedded.inline.sql": "sql"
            }
         }
      ],
      "commands": [
         {
            "command": "stack.togglePostgreSQL",
            "title": "Заменяет тильды в запросе и наоборот",
            "category": "stack"
         }
      ],
      "keybindings": [
         {
            "command": "stack.togglePostgreSQL",
            "key": "f12",
            "mac": "f12",
            "when": "editorTextFocus"
         }
      ],
      "configuration": {
         "type": "object",
         "title": "stack-lang-server",
         "properties": {}
      },
      "snippets": [
         {
            "language": "stack",
            "path": "./snippets/snippets.json"
         }
      ]
   },
   "scripts": {
      "postinstall": "cd client && npm i",
      "server:compile": "server-compile.sh",
      "client:compile": "tsc -b",
      "watch": "rm -rf client/out && tsc -b -w",
      "package": "vsce package",
      "publish": "vsce publish"
   },
   "dependencies": {
         "vscode-languageclient": "8.0.1"
   },
   "devDependencies": {
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.0",
      "@types/node": "^12.12.0",
      "@typescript-eslint/eslint-plugin": "^3.8.0",
      "@typescript-eslint/parser": "^3.8.0",
      "cross-env": "^7.0.2",
      "@electron/rebuild": "3.7.2",
      "eslint": "^7.6.0",
      "glob": "^7.1.7",
      "mocha": "^8.0.1",
      "ts-loader": "^9.5.1",
      "typescript": "^5.4.5",
      "@vscode/test-electron": "^1.4.0",
      "vscode-uri": "^3.0.2",
      "webpack": "^5.82.1",
      "webpack-cli": "^5.1.4"
   }
}
